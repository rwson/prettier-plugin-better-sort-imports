{
    "name": "prettier-plugin-better-sort-imports",
    "version": "1.1.0",
    "description": "A better prettier plugins to sort imports in provided RegEx order",
    "main": "lib/src/index.js",
    "repository": {
        "url": "https://github.com/rwson/prettier-plugin-better-sort-imports",
        "type": "git"
    },
    "homepage": "https://github.com/rwson/prettier-plugin-better-sort-imports#readme",
    "scripts": {
        "prepare": "yarn run compile",
        "compile": "tsc",
        "preexample": "yarn run compile",
        "example": "prettier --config ./examples/.prettierrc --plugin lib/src/index.js --write './examples/**/*.{js,jsx,ts,tsx}'",
        "type-check": "tsc --noEmit",
        "prepublishOnly": "npm run compile",
        "linkLocal": "npm run compile && npm link"
    },
    "keywords": [
        "prettier",
        "plugin",
        "sort",
        "import",
        "typescript",
        "javascript"
    ],
    "author": {
        "name": "rwson",
        "email": "rwson92@gmail.com",
        "url": "https://github.com/rwson"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "@babel/core": "7.13.10",
        "@babel/generator": "7.13.9",
        "@babel/parser": "7.14.6",
        "@babel/traverse": "7.13.0",
        "@babel/types": "7.13.0",
        "javascript-natural-sort": "0.7.1",
        "lodash": "4.17.21"
    },
    "devDependencies": {
        "@types/chai": "4.2.15",
        "@types/jest": "26.0.20",
        "@types/lodash": "4.14.168",
        "@types/node": "14.14.34",
        "prettier": "2.3.1",
        "typescript": "4.2.3"
    },
    "peerDependencies": {
        "prettier": "2.x"
    }
}
